<!-- playervideo.dtl -->
<div class="headerbg">
    <div class="container">
        <div class="topAd">
          <script src="http://tag.contextweb.com/TagPublish/getjs.aspx?action=VIEWAD&cwrun=200&cwadformat=728X90&cwpid=557677&cwwidth=728&cwheight=90&cwpnet=1&cwtagid=205117"></script>
        </div>
        <header>
        	<a href="/" class="logo" title="Ezyfashion"><img src="/images/logo.png" alt="" /></a>
            <nav>
                <div class="menu">
                    <span>MENU</span>
                    <ul class="Navigation">
                       <li><a href="/" title="Home">Home</a></li>
                        <li><a href="/morevideos" class="active" title="Latest Videos">Latest Videos</a></li>
                        <li><a href="/morefashiontips" title="People">People</a></li>
                        <li><a href="/movienews" title="Movie News">Movie News</a> |</li>
                        <li><a href="/moremusic" title="Music">Music</a></li>
                    </ul>
                </div>
            </nav>
        	<span class="clear"></span>
        </header>
    </div>
</div>
<!-- headerbg end -->  
	  
<div class="slideshow">
    <div class="container">
        <div class="slide">
        <div class="Title yellow">{{ newsParam.title }}<span style="color:#000; font-size:13px"> - {{newsParam.upload_date}}</span></div>
            <input type="hidden" name="vide_val" id="video_val" value="{{newsParam.video}}">
            <div id="trailor">Loading the player...</div>
            <!-- <div><p>{{ newsParam.description }}</p></div> -->
        </div>       
    </div>
     <!-- <div class="Title red">{{ newsParam.title }}</div>
     <div><p>{{ newsParam.description }}</p></div> -->
    </div>
</div>
<!-- slideshow end -->		  

<div class="container">
	<section class="contentarea">
		<aside class="colum1">
    <div class="rightad">
        <script src="http://tag.contextweb.com/TagPublish/getjs.aspx?action=VIEWAD&cwrun=200&cwadformat=300X250&cwpid=557677&cwwidth=300&cwheight=250&cwpnet=1&cwtagid=205116"></script>
      </div>
      <div class="module">
      <div class="Title yellow">People</div>
        <!-- {% for n in Models1 %}
          <div class="picture">
            <a href="/more/{{ n.id }}" title="{{ n.title }}">
              <img src="http://newscdn.lycos.com/{{ n.thumb_image }}" alt="" title="{{ n.title }}"/>
            </a>
          </div>
          <div class="description"><h3>{{ n.title }}</h3>
          </div>   
        {% endfor %} -->

        
        <div class="gallery">
          {% for n in Models %}
            <div class="ng-scope">
              <a href="/more/{{ n.id }}">
                <!-- <img src="http://newscdn.lycos.com/{{ n.thumb_image }}" alt="" title="{{ n.title }}"/> -->
                <div class="description">{{n.title}}</div>
                <span class="date">{{n.upload_date}}</span>
              </a>
            </div> 
          {% endfor %}       
        </div>      
      <a href="/morefashiontips" class="more">Check more</a>
    </div>
		<!-- module end -->
		</aside>
		<!-- colum1 end -->
	  
        <aside class="colum2">
            <!-- <div class="module">
            
                <div class="Title red">{{ newsParam.title }}</div>                
                <div class="module-inner-videos">
                  <div id="play_video"></div>                                          
                  <div >
                      <p>{{ newsParam.description }}</p>
                  </div>                   
                </div> 
                         
            </div> -->
            <!-- People end --> 

            <div class="module">
              <div class="Title yellow">More Videos</div>
                <div class="row">
                {% for n in latestPopularVideos %}
                  <div class="video pvideos">
                    <!-- <div class="video" ng-repeat="n in gallery1"> -->
                      <a href="/playvideo/{{ n.id }}">
                        <span class="play"></span>  
                        <img src="http://newscdn.lycos.com/world_news/{{ n.thumb_image }}" alt="" />
                        <div class="span6">
                      <h3>{{ n.title }}</h3>
                      <span class="date">{{ n.upload_date }}</span>
                  </div>
                      </a>
                    </div>

                {% endfor %} 
                    <span class="clear"></span>
                    <a href="/morevideos" class="more">More Videos</a>
                  
                </div>
              </div>

             <div class="module">
              <div class="Title yellow">Music</div>
                <div class="row">
                {% for n in gallery %}
                  <div class="video">
                    <!-- <div class="video" ng-repeat="n in gallery1"> -->
                      <a href="/more/{{n.value.id}}"> <img src="http://newscdn.lycos.com/{{ n.value.thumb_image }}" alt="" /></a>
                  </div> 
                  
                {% endfor %} 
                    <span class="clear"></span>
                    <a href="/moremusic" class="more">More Music</a>
                  
                </div>
              </div>

    </aside>
		<!-- colum2 end -->
		
		<aside class="colum3">
			<div class="rightad">
        <script src="http://tag.contextweb.com/TagPublish/getjs.aspx?action=VIEWAD&cwrun=200&cwadformat=300X250&cwpid=557677&cwwidth=300&cwheight=250&cwpnet=1&cwtagid=205116"></script>
      </div>

          <div class="module">
             <div class="Title yellow">Movie News</div>
                <div class="module-inner">
                  <div class="populargasip" >
                    {% for n in latestbeautynews %}
                      <div class="ng-scope">                      
                          <!-- <span class="span4" style="position: relative;">
                              <a href="/more/{{ n.id }}">   
                                <img src="http://newscdn.lycos.com/{{ n.thumb_image }}" alt="" />
                              </a>
                          </span> -->
                          <div>                              
                                <a href="/more/{{ n.id }}"><h3>{{ n.title }}</h3></a>
                              
                              <span class="date">{{ n.upload_date }}</span>
                          </div>                          
                      </div>
                    {% endfor %}
                  </div>                    
                    <a href="/morevideos" class="more">More Popular Videos</a>
                </div>
          </div>

			<div class="twittertweets margin15_T">
		  <div class="twitterAdd">
        <script src="http://tag.contextweb.com/TagPublish/getjs.aspx?action=VIEWAD&cwrun=200&cwadformat=160X600&cwpid=557677&cwwidth=160&cwheight=600&cwpnet=1&cwtagid=205115"></script>
      </div>

		</div><!-- twittertweets -->
        
            
            

		</aside>
		<!-- colum3 end -->
	  
	  
    
	</section>
    <span class="clear"></span>
    
    <div class="bottomad">
      <script src="http://tag.contextweb.com/TagPublish/getjs.aspx?action=VIEWAD&cwrun=200&cwadformat=728X90&cwpid=557677&cwwidth=728&cwheight=90&cwpnet=1&cwtagid=205117"></script>
    </div>

   <div class="footer">
        <p>
            <a href="/">Home</a> | 
            <a href="/morevideos">Latest Videos</a> | 
            <a href="/morefashiontips">People</a> | 
            <a href="/movienews" title="Movie News">Movie News</a> |
            <a href="/moremusic">Music</a> | 
            <a href="/termsandconditions">Terms & Conditions</a></p>
        <p>Copyright &copy; {% now "Y" %}  Ezyfashion. All Rights Reserved.</p>
    </div>

</div>
