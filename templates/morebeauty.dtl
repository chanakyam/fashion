        <div class="gallery">
        {% for n in Models %}
        <div class="ng-scope">
          <a href="/more/{{ n.value.id }}">
            <!-- <img src="http://img1.contentapi.ws/{{ n.thumb_image }}" alt="" title="{{ n.title }}"/> -->
            <div class="description">{{ n.value.title }}</div>
            <span class="date">{{n.value.upload_date}}</span>
          </a>
          </div> 
          {% endfor %}       
        </div>
      
      <a href="/morefashiontips" class="more">Check more</a>
    </div>
    
    <!-- module end -->
    </aside>
    <!-- colum1 end -->
    
        <aside class="colum2">  
          <div class="module">
